{
  "name": "my-drill-app",
  "version": "1.0.0",
  "description": "Koa.js api server",
  "main": "server.js",
  "scripts": {
    "test": "NODE_ENV=test mocha",
    "start": "node server.js",
    "dev": "nodemon server.js",
    "dev:cloud": "NODE_ENV=production npm run dev",
    "deploy": "git push heroku master",
    "deploy:create": "heroku create && npm run set:env && npm run deploy",
    "set:env": "source .heroku-env && heroku config:set ELASTIC_CLOUD_ID=$ELASTIC_CLOUD_ID ELASTIC_CLOUD_USERNAME=$ELASTIC_CLOUD_USERNAME ELASTIC_CLOUD_PASSWORD=$ELASTIC_CLOUD_PASSWORD"
  },
  "engines": {
    "node": "14.x"
  },
  "keywords": [],
  "author": "Arsalan Sadri",
  "license": "ISC",
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "dotenv": "^8.2.0",
    "if-env": "^1.0.4",
    "koa": "^2.13.1",
    "koa-body": "^4.2.0",
    "koa-logger": "^3.2.1",
    "koa-router": "^10.0.0"
  },
  "devDependencies": {
    "chai": "^4.2.0",
    "mocha": "^8.2.1",
    "supertest": "^6.0.1"
  }
}
